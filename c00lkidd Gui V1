-- üî• Fun / Troll
addButton("tubers93 Jumpscare (Fake)", function()
    loadstring(game:HttpGet("https://rawscripts.net/raw/Universal-Script-jumpscare-tubers93-24644"))()
end)

addButton("Disco", function()
    while task.wait(0.2) do
        Lighting.Ambient = Color3.new(math.random(), math.random(), math.random())
    end
end)

addButton("Explode All", function()
    for _,plr in pairs(Players:GetPlayers()) do
        if plr.Character then
            Instance.new("Explosion", plr.Character).Position = plr.Character.HumanoidRootPart.Position
        end
    end
end)

addButton("666", function()
    for _,part in pairs(Workspace:GetDescendants()) do
        if part:IsA("BasePart") then
            local fire = Instance.new("Fire", part)
            fire.Size = 15
            fire.Heat = 10
        end
    end
end)

