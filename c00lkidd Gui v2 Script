addButton("Project c0end2027", function()
    -- Run the script
    loadstring(game:HttpGet("https://pastebin.com/raw/8yAEWSVE"))()

    -- Show notification with key
    local notifGui = Instance.new("ScreenGui", player:WaitForChild("PlayerGui"))
    notifGui.Name = "c0end2027Notif"
    notifGui.ResetOnSpawn = false

    local notif = Instance.new("TextLabel", notifGui)
    notif.Size = UDim2.new(0, 220, 0, 50)
    notif.Position = UDim2.new(0.5, -110, 0.1, 0)
    notif.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    notif.BorderSizePixel = 2
    notif.BorderColor3 = Color3.fromRGB(0, 255, 0)
    notif.Text = "Key Is: c0end2027"
    notif.TextColor3 = Color3.fromRGB(0, 255, 0)
    notif.Font = Enum.Font.Arcade
    notif.TextSize = 18

    task.delay(7, function()
        if notifGui then
            notifGui:Destroy()
        end
    end)
end)

